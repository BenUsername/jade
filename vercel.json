{
    "version": 2,
    "buildCommand": "npm install --only=production && npm run build",
    "functions": {
        "api/**/*": {
            "runtime": "nodejs20.x",
            "memory": 1024,
            "maxDuration": 60
        }
    },
    "routes": [
        { "src": "/(.*)", "dest": "/$1" }
    ]
}